    <!-- AI Workflows Section -->
    <section id="workflows" class="ai-workflows">
        <div class="container">
            <div class="section-header">
                <h2 class="section-title">Two Powerful AI Solutions</h2>
                <p class="section-subtitle">Complete legal document analysis and generation platform</p>
            </div>
    
            <div class="workflows-grid">
                <!-- AI 1: Document Analysis (Merged) -->
                <div class="workflow-card analysis-card" data-aos="fade-up" data-aos-delay="100">
                    <div class="card-glow"></div>
                    <div class="workflow-icon">
                        <i class="fas fa-file-search"></i>
                    </div>
                    <h3>Taर्क View</h3>
                    <p>Comprehensive legal document analysis combining summary generation, risk assessment, and key terms extraction in one powerful workflow.</p>
                    
                    <ul class="workflow-features">
                        <li>Executive summary generation</li>
                        <li>Risk assessment and liability scoring</li>
                        <li>Key terms and clause identification</li>
                        <li>Compliance gap analysis</li>
                        <li>Multi-format document support</li>
                    </ul>
    
                    <div class="workflow-interface">
                        <form id="upload-form" action="/analyze_document" method="POST" enctype="multipart/form-data">
                            <div class="upload-area" id="analysis-upload">
                              <i class="fas fa-cloud-upload-alt"></i>
                              <p id="upload-text">Drop document here or click to upload</p>
                              <input class="file_upload" type="file" name="pdf" id="analysis-file" accept=".pdf,.doc,.docx" hidden>
                            </div>
                            <div class="center_div">
                                <button type="submit" class="cta-button primary">
                                    <span>Analyze Document</span>
                                    <i class="fas fa-arrow-right"></i>
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
    
                <!-- AI 2: Contract Generator -->
                <div class="workflow-card generator-card" data-aos="fade-up" data-aos-delay="200">
                    <div class="card-glow"></div>
                    <div class="workflow-icon">
                        <i class="fas fa-file-contract"></i>
                    </div>
                    <h3>Taर्क Assist</h3>
                    <p>Professional contract creation with customizable templates for various business and legal scenarios.</p>
                    
                    <ul class="workflow-features">
                        <li>Service agreement templates</li>
                        <li>Employment contracts</li>
                        <li>Non-disclosure agreements</li>
                        <li>Partnership agreements</li>
                        <li>Custom clause insertion</li>
                    </ul>
    
                    <div class="workflow-interface max-w-3xl mx-auto p-6 center_div">
                        <form action="/generate_contract" method="post">
                            <button type="submit" class="cta-button primary">
                                <span>Generate pdf</span>
                                <i class="fas fa-arrow-right"></i>
                            </button>
                        </form>
                      </div>
                      
                        
                        <div class="result-area" id="contract-result"></div>
                    </div>
                </div>
            </div>
        </div>
    </section>

